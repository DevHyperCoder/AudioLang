{
    "id" : "com.devhypercoder.audiolang",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "45",
    "sdk" : "org.gnome.Sdk",
    "command" : "audiolang",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=pulseaudio",
        "--device=dri",
        "--socket=wayland"
    ],
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
	"python3-requests.json",
	"blueprint-compiler.json",
        {
            "name" : "audiolang",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "git",
		    "path": ".",
		    "branch":"HEAD"
                }
            ]
        }
    ]
}
